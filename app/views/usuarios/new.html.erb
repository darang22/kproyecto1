<div class = "form">
    <h1>Crear cuenta</h1>
    <%= form_for(@usuario) do |f| %>
        <%= f.text_field :nombre, placeholder: "Nombre"%>
        <%= f.email_field :email, placeholder: "Correo" %>
        <%= f.password_field :password, placeholder: "Password" %>
        <%= f.submit "Crear usuario" %>
        <% end %>
</div>